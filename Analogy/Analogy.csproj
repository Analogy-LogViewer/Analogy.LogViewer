<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyName>Analogy</AssemblyName>
    <RootNamespace>Analogy</RootNamespace>
    <ApplicationIcon>Icons\Analogy_icon1.ico</ApplicationIcon>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageId>Analogy.LogViewer.Core</PackageId>
    <Authors>Lior Banai</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Analogy-LogViewer/Analogy.LogViewer</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/Analogy-LogViewer/Analogy.LogViewer</RepositoryUrl>
    <PackageTags />
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Analogy.DataProviders.Extensions" Version="1.0.4" />
    <PackageReference Include="Analogy.LogViewer.Example" Version="1.1.0" />
    <PackageReference Include="Analogy.LogViewer.Interfaces" Version="2.1.8" />
    <PackageReference Include="Analogy.LogViewer.NLogProvider" Version="1.0.5" />
    <PackageReference Include="Analogy.LogViewer.Serilog" Version="1.0.0" />
    <PackageReference Include="MessagePack" Version="2.1.90" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="MainForm.resx">
      <DependentUpon>MainForm.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="DevExpress.Charts.v19.1.Core">
      
    </Reference>
    <Reference Include="DevExpress.Data.v19.1">
      
    </Reference>
    <Reference Include="DevExpress.Images.v19.1">
      
    </Reference>
    <Reference Include="DevExpress.Printing.v19.1.Core, Version=19.1.9.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a, processorArchitecture=MSIL">
      <SpecificVersion>False</SpecificVersion>
      
    </Reference>
    <Reference Include="DevExpress.Utils.v19.1, Version=19.1.9.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a, processorArchitecture=MSIL">
      <SpecificVersion>False</SpecificVersion>
      
    </Reference>
    <Reference Include="DevExpress.XtraBars.v19.1">
      
    </Reference>
    <Reference Include="DevExpress.XtraCharts.v19.1">
      
    </Reference>
    <Reference Include="DevExpress.XtraCharts.v19.1.UI">
      
    </Reference>
    <Reference Include="DevExpress.XtraCharts.v19.1.Wizard">
      
    </Reference>
    <Reference Include="DevExpress.XtraEditors.v19.1">
      
    </Reference>
    <Reference Include="DevExpress.XtraGrid.v19.1">
      
    </Reference>
    <Reference Include="DevExpress.XtraLayout.v19.1">
      
    </Reference>
    <Reference Include="DevExpress.XtraPrinting.v19.1">
      
    </Reference>
    <Reference Include="DevExpress.XtraTreeList.v19.1">
      
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\icon.png" Link="Icons\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>